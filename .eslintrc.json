{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-var": "off",
    "no-unused-vars": ["warn", { "args": "none", "vars": "all" }],
    "no-console": "off"
  }
}
